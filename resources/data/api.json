    "components": {
        "parameters": {
            "SlugInPath": {
                "in": "path",
                "required": true,
                "name": "website",
                "schema": {
                    "$ref": "#/components/schemas/WebsiteSlug"
                }
            },
            "FiscalNumberInPath": {
                "in": "path",
                "required": true,
                "name": "fn",
                "schema": {
                    "$ref": "#/components/schemas/FiscalNumber"
                }
            }
        },
        "schemas": {
            "WebsiteSlug": {
                "type": "string",
                "pattern": "^[a-z0-9]+(?:-[a-z0-9]+)*$"
            },
            "WebsiteType": {
                "type": "string",
                "description": "Tipologia di sito",
                "enum": [
                    "institutional website",
                    "informational or thematic website",
                    "services website",
                    "mobile application"
                ]
            },
            "WebsiteObject": {
                "type": "object",
                "required": ["website_name", "url", "type"],
                "properties": {
                    "website_name": {
