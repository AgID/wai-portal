{
  "type": "object",
  "required": [
    "uniqueId",
    "referencePeriod",
    "transferDate",
    "transferType",
    "nbEntries",
    "sources"
  ],
  "properties": {
    "uniqueId": {
      "description": "Unique ID for feedback submission for a specific reference period collected from the Unique ID web API call.",
      "type": "string"
    },
    "referencePeriod": {
      "$ref": "referencePeriod.json"
    },
    "transferDate": {
      "format": "date-time",
      "type": "string"
    },
    "transferType": {
      "type": "string",
      "enum": [
        "API",
        "Manual"
      ]
    },
    "nbEntries": {
      "type": "number"
    },
    "sources": {
      "type": "array",
      "items": {
        "$ref": "source.json"
      }
    }
  },
  "$schema": "http://json-schema.org/schema#"
}