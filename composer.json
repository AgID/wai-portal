{
    "name": "italia/analytics",
    "description": "Web Analytics Italia",
    "version": "1.4.0-beta",
    "keywords": ["analytics", "italia"],
    "license": "AGPLv3+",
    "type": "project",
    "authors": [
        {
            "name": "Davide Porrovecchio",
            "homepage": "https://github.com/pdavide",
            "role": "Developer"
        },
        {
            "name": "Andrea Capelli",
            "homepage": "https://github.com/valair",
            "role": "Developer"
        },
        {
            "name": "Daniela De Blasis",
            "homepage": "https://github.com/nikorobins",
            "role": "UX/UI Designer"
        },
        {
            "name": "Daniela Iozzo",
            "homepage": "https://github.com/danielaiozzo",
            "role": "Content Designer/Web analytics specialist"
        },
        {
            "name": "Michela Calanna",
            "homepage": "https://github.com/GoldDustWoman",
            "role": "UX/UI Designer"
        }
    ],
    "require": {
        "php": "^7.3",
        "bensampo/laravel-enum": "^1.25",
        "davejamesmiller/laravel-breadcrumbs": "^5.3",
        "davidepastore/codice-fiscale": "^0.7.0",
        "elasticsearch/elasticsearch": "7.6.*",
        "ethanhann/redisearch-php": "^1.0",
        "fideloper/proxy": "^4.2",
        "guzzlehttp/guzzle": "^6.3",
        "italia/spid-laravel": "^1.0@beta",
        "laravel/framework": "6.16.*",
        "laravel/tinker": "^2.0",
        "league/commonmark": "^1.2",
        "league/commonmark-ext-external-link": "^1.0",
        "predis/predis": "^1.1",
        "silber/bouncer": "v1.0.0-rc.8",
        "spatie/laravel-webhook-client": "^2.3",
        "symfony/yaml": "^4.0",
        "yajra/laravel-datatables-fractal": "^1.0",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2.7",
        "beyondcode/laravel-dump-server": "^1.3",
        "facade/ignition": "^1.4",
        "friendsofphp/php-cs-fixer": "^2.14",
        "fzaninotto/faker": "^1.4",
        "johnkary/phpunit-speedtrap": "^3.1",
        "laravel/dusk": "^6.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phing/phing": "2.*",
        "phpunit/phpunit": "^9.0",
        "sensiolabs/security-checker": "^6.0",
        "shaggy8871/php-rx": "^1.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        },
        "enable-patching": "true"
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
