{
    "name": "italia/wai-portal",
    "description": "Web Analytics Italia",
    "version": "2.7.2-beta",
    "keywords": ["analytics", "italia"],
    "license": "AGPL-3.0-or-later",
    "type": "project",
    "authors": [
        {
            "name": "Davide Porrovecchio",
            "homepage": "https://github.com/pdavide",
            "role": "Developer"
        },
        {
            "name": "Andrea Capelli",
            "homepage": "https://github.com/valair",
            "role": "Developer"
        },
        {
            "name": "Daniela De Blasis",
            "homepage": "https://github.com/nikorobins",
            "role": "UX/UI Designer"
        },
        {
            "name": "Daniela Iozzo",
            "homepage": "https://github.com/danielaiozzo",
            "role": "Content Designer/Web analytics specialist"
        },
        {
            "name": "Michela Calanna",
            "homepage": "https://github.com/GoldDustWoman",
            "role": "UX/UI Designer"
        }
    ],
    "require": {
        "php": "^7.4",
        "bensampo/laravel-enum": "^1.25",
        "davidepastore/codice-fiscale": "^0.9.0",
        "diglactic/laravel-breadcrumbs": "^7.0",
        "doctrine/dbal": "^2.10",
        "elasticsearch/elasticsearch": "8.5.*",
        "ethanhann/redisearch-php": "^1.0",
        "fideloper/proxy": "^4.2",
        "gmponos/guzzle_logger": "^2.0",
        "guzzlehttp/guzzle": "^7.0",
        "italia/spid-laravel": "^1.1.0-beta",
        "justinrainbow/json-schema": "^5.2",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "league/commonmark": "^1.4",
        "predis/predis": "^1.1",
        "silber/bouncer": "v1.0.0",
        "spatie/laravel-webhook-client": "^2.3",
        "symfony/yaml": "^4.0",
        "yajra/laravel-datatables-fractal": "^1.0",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2.7",
        "beyondcode/laravel-dump-server": "^1.3",
        "facade/ignition": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "fzaninotto/faker": "^1.4",
        "johnkary/phpunit-speedtrap": "^4.0",
        "laravel/dusk": "^6.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.3",
        "phing/phing": "2.*",
        "phpunit/phpunit": "^9.0",
        "shaggy8871/php-rx": "^1.0",
        "symfony/thanks": "^1.0",
        "psr/event-dispatcher": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        },
        "enable-patching": "true"
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "symfony/thanks": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
