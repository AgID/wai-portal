{
    "name": "italia/analytics",
    "description": "Web Analytics Italia",
    "keywords": ["analytics", "italia"],
    "license": "AGPLv3+",
    "type": "project",
    "authors": [
        {
            "name": "Davide Porrovecchio - Agenzia per l'Italia Digitale",
            "email": "davide.porrovecchio@agid.gov.it"
        }
    ],
    "require": {
        "php": "^7.3",
        "davejamesmiller/laravel-breadcrumbs": "^5.0",
        "ethanhann/redisearch-php": "^1.0",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "italia/spid-laravel": "^0.2.0@beta",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "league/fractal": "^0.17.0",
        "predis/predis": "^1.1",
        "silber/bouncer": "v1.0.0-rc.5",
        "symfony/yaml": "^4.0",
        "usu/codice-fiscale": "^1.0",
        "yajra/laravel-datatables-fractal": "^1.0",
        "yajra/laravel-datatables-oracle": "~9.0",
        "bensampo/laravel-enum": "^1.17"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "fzaninotto/faker": "^1.4",
        "johnkary/phpunit-speedtrap": "^3.1",
        "laravel/dusk": "^5.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phing/phing": "2.*",
        "phpunit/phpunit": "^8.1",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        },
        "enable-patching": "true"
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
